<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Angular Webpack ES6 Project</title>
    <meta name="description" content="">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <link rel="shortcut icon" href="favicon.ico" type="image/x-icon">
    <% for(var i=0; i < htmlWebpackPlugin.files.css; i++) { %>
        <link href="<%= htmlWebpackPlugin.files.css[i] %>" rel="stylesheet">
    <% } %>

    <% for(var i=0; i < htmlWebpackPlugin.files.chunks; i++) { %>
        <script src="<%= htmlWebpackPlugin.files.chunks[i].entry %>" defer></script>
    <% } %>
</head>
<body>
    <body ng-app='app'>
        <nav class="navbar navbar-light bg-secondary">
            <form class="form-inline">
                <button class="btn btn-outline-success home" ui-sref="home" ui-sref-active="active" type="button">Home</button>
                <button class="btn btn-outline-success converter" ui-sref="converter" ui-sref-active="active" type="button">Converter</button>
            </form>
        </nav>

    <div ui-view></div>

    <footer-test></footer-test>

</body>
</html>